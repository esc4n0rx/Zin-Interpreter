{
    "programa": {
        "nome": "CALCULADORA",
        "variaveis": [
            {
                "nome": "numero1",
                "tipo": "inteiro"
            },
            {
                "nome": "numero2",
                "tipo": "inteiro"
            },
            {
                "nome": "resultado",
                "tipo": "inteiro"
            }
        ],
        "implementacao": {
            "principal": [
                {
                    "pergunte": {
                        "texto": "Digite o primeiro número:",
                        "variavel": "numero1"
                    }
                },
                {
                    "pergunte": {
                        "texto": "Digite o segundo número:",
                        "variavel": "numero2"
                    }
                },
                {
                    "executar_modulo": "OPERACOES"
                },
                {
                    "escreva": "O cálculo foi concluído."
                }
            ],
            "execucoes_apos_principal": [],
            "modulos": {
                "OPERACOES": [
                    {
                        "nome": "calcular",
                        "parametros": [
                            "numero1",
                            "numero2"
                        ],
                        "corpo": [
                            {
                                "tipo": "SE",
                                "condicao": {
                                    "left": "numero1",
                                    "operator": ">",
                                    "right": "numero2"
                                },
                                "bloco_se": [
                                    {
                                        "atribuir": {
                                            "variavel": "resultado",
                                            "valor": {
                                                "left": "numero1",
                                                "operator": "-",
                                                "right": "numero2"
                                            }
                                        }
                                    },
                                    {
                                        "escreva": "O resultado é {resultado}."
                                    }
                                ],
                                "bloco_senao": [
                                    {
                                        "atribuir": {
                                            "variavel": "resultado",
                                            "valor": {
                                                "left": "numero1",
                                                "operator": "+",
                                                "right": "numero2"
                                            }
                                        }
                                    },
                                    {
                                        "escreva": "O resultado é {resultado}."
                                    }
                                ]
                            }
                        ],
                        "retorno": "resultado"
                    }
                ]
            }
        },
        "execucao": {
            "modulos": [
                "PRINCIPAL"
            ]
        }
    }
}